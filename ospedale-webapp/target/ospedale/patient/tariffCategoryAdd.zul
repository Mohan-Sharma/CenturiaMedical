   //  List ptntCategory = Arrays.asList(arr);
   //  List arr=ptntTariff.PatientCategory.values();
   //  Arrays.asList(arr);

  //  com.nzion.domain.billing.TariffCategory.PatientCategory[]  arr = com.nzion.domain.billing.TariffCategory.PatientCategory.values();
