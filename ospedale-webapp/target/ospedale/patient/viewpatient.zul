    <zscript>
        <![CDATA[
        public void displayDemographicDetailsAccordingToConsent(com.nzion.domain.Patient patient){
            Boolean demographicDetailsVerified = getPatientPrivacyPolicyConsentByCriteria(consents, "I have verified my Demographic details and confirm they are correct.");
            if(!demographicDetailsVerified){
                displayConsentVerificationMsg1.setVisible(Boolean.TRUE);
                displayConsentVerificationMsg2.setVisible(Boolean.TRUE);
                displayConsentVerificationMsg3.setVisible(Boolean.FALSE);
                displayConsentVerificationMsg4.setVisible(Boolean.FALSE);
                patientInfo.setOpen(true);
                contactInfo.setOpen(true);
            } else{
                displayConsentVerificationMsg1.setVisible(Boolean.FALSE);
                displayConsentVerificationMsg2.setVisible(Boolean.FALSE);
                displayConsentVerificationMsg3.setVisible(Boolean.TRUE);
                displayConsentVerificationMsg4.setVisible(Boolean.TRUE);
                patientInfo.setOpen(true);
                contactInfo.setOpen(true);
            }
        }

        ]]>
